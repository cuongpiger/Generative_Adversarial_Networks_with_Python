                <mxCell id="11" value="How it works?" style="edgeLabel;html=1;align=center;verticalAlign=middle;resizable=0;points=[];" parent="9" vertex="1" connectable="0">
                    <mxGeometry x="0.1221" relative="1" as="geometry">
